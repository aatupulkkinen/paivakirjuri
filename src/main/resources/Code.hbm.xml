<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="otp.model.entities.Code" table="recovery_codes" schema="test_eng">
        <id name="name">
            <column name="name" sql-type="varchar(40)"/>
        </id>
        <property name="code">
            <column name="code" sql-type="varchar(12)" length="12"/>
        </property>
    </class>
</hibernate-mapping>