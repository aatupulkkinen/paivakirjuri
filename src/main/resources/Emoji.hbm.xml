<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="otp.model.entities.Emoji" table="emojis" schema="test_eng">
        <id name="emojiId">
            <column name="emoji_id" sql-type="int(11)"/>
        </id>
        <property name="path">
            <column name="path" sql-type="varchar(64)" length="64"/>
        </property>
    </class>
</hibernate-mapping>